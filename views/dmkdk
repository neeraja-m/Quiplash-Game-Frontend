<div v-else-if="gameState.state === 0">
                <!-- SAME AS PLAYERS SEE -->
                <%- include('lobby'); -%>
            </div>
            <div v-else-if="gameState.state ===1 ">
                <!-- DISPLAY WHICH PLAYERS HAVE FINISHED SUBMITTING -->
                <%- include('submitPromptDisplay'); -%>
            </div>
            <div v-else-if="gameState.state == 2 ">
                <!-- DISPLAY WHICH PLAYERS HAVE FINISHED SUBMITTING -->
                <%- include('answerPromptDisplay') -%>
            </div>
            <div v-else-if="gameState.state == 3">
                <!-- DISPLAY WHICH PLAYERS HAVE FINISHED VOTING -->
                <%- include('votingDisplay') -%>
            </div>
            <div v-else-if="gameState.state == 4">
                <!-- SAME AS PLAYERS SEE -->
                <%- include('revealVotes') -%>
            </div>
            <div v-else-if="gameState.state == 5">
                <!-- SAME AS PLAYERS SEE -->
                <%- include('roundScoreboard') -%>
            </div>
            <div v-else-if="gameState.state == 6">
                <!-- SAME AS PLAYERS SEE -->
                <%- include('finalScoreboard') -%>
            </div>